import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        long a = sc.nextLong();
        int b = sc.nextInt();
        long sum = 1;
        while(b>0){
            sum = sum*a;
            b--;
        }
        System.out.println(sum);
    }
}
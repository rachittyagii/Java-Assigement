import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int absN=Math.abs(n);
        int a=absN/100;
        int b=(absN%100)/10;
        int c=absN%10;
        System.out.println(a);
        System.out.println(b);
        System.out.println(c);
    }
}